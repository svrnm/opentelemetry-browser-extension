{
   "manifest_version": 3,
   "permissions": [
      "activeTab", 
      "scripting",
      "webNavigation",
      "storage"
   ],
   "action": {
      "default_popup": "popup.html"
   },
   "icons": "icons/otel-logo_{size}.png",
   "background": {
       "service_worker": "background.js"
   },
   "options_page": "options.html",
   "web_accessible_resources": [
      {
         "resources": [
            "instrumentation.js"
            ],
         "matches": [
            "<all_urls>"
         ]  
      }
   ]
}